{
    "name": "snap/analytics",
    "description": "Google Analytics.",
    "keywords": ["snap", "google analytics", "analytics"],
    "homepage": "http://thedaylightstudio.com",
    "license": "proprietary",
    "type": "project",
    "authors": [
        {
            "name": "David McReynolds",
            "email": "dave@bydaylight.com"
        }
